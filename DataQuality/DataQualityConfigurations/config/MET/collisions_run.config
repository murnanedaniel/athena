###########################
# Algorithms 
###########################

algorithm METGatherData {
    libname = libdqm_algorithms.so
    name = GatherData
    reference = stream=physics_Main:CentrallyManagedReferences_Main;CentrallyManagedReferences
}

algorithm METGatherDataRndm {
    libname = libdqm_algorithms.so
    name = GatherData
    reference = stream=physics_Main:CentrallyManagedReferences_Main;CentrallyManagedReferences
}

compositeAlgorithm METGatherData&SideBand {
    libnames = libdqm_algorithms.so
    subalgs = GatherData,SideBand_Relative
}

algorithm METSideBand8 {
    name = METGatherData&SideBand
    reference = stream=physics_Main:CentrallyManagedReferences_Main;CentrallyManagedReferences
    UseUnderFlow = 1
    UseOverFlow    = 1
    Min = -8000.0
    Max = +8000.0
    thresholds = METSideBandThres
}

algorithm METSideBand20 {
    name = METGatherData&SideBand
    reference = stream=physics_Main:CentrallyManagedReferences_Main;CentrallyManagedReferences
    UseUnderFlow = 1
    UseOverFlow    = 1
    Min = -20000.0
    Max = +20000.0
    thresholds = METSideBandThres
}

algorithm METSideBand80 {
    name = METGatherData&SideBand
    reference = stream=physics_Main:CentrallyManagedReferences_Main;CentrallyManagedReferences
    UseUnderFlow = 1
    UseOverFlow    = 1
    Min = -80000.0
    Max = +80000.0
    thresholds = METSideBandThres
}

